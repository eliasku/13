{
  "name": "@eliasku/js13k-2022",
  "version": "1.0.10",
  "description": "js13k game with p2p realtime multiplayer",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "build": "ts-node --esm --swc tools/build.ts",
    "watch": "ts-node --esm --swc tools/watch.ts",
    "clean": "ts-node --esm --swc tools/clean.ts",
    "typecheck-client": "tsc -p client/tsconfig.json --noEmit",
    "typecheck-server": "tsc -p server/tsconfig.json --noEmit"
  },
  "author": "eliasku",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.22",
    "@types/node": "^18.11.14",
    "esbuild": "^0.16.7",
    "html-minifier": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
